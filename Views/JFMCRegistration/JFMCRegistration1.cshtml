    function EditJFMC(Id) {
        $('#hdnId').val(Id);
        var Id = { Id: Id }
        $.ajax({
            type: 'POST',
            url: RootUrl + 'JFMCRegistration/Edit', // we are calling json method
            contentType: 'application/json; charset=utf-8',
            data: JSON.stringify(Id),
            success: function (data) {
                $('#Dist_Code').val(data.Dist_Code);
                Block(data.Dist_Code,data.Blk_Code);              
                GP(data.Dist_Code,data.Blk_Code, data.Gp_Code);
                Village(data.Dist_Code, data.Blk_Code, data.Gp_Code, data.Vill_Code);
                $('#Minutes_of_Meeting').val(data.Minutes_of_Meeting);
                $('#Committee_Name').val(data.Committee_Name);
                $('#Registration_Id').val(data.Registration_Id);
                $('#Secretary_Name').val(data.Secretary_Name);
                $('#Lattitude').val(data.Lattitude);
                $('#Longitude').val(data.Longitude);
                $('#Plantation_Area').val(data.Plantation_Area);
                $('#Total_Area_Managed').val(data.Total_Area_Managed);
                $('#Bank_Account_Operated').val(data.Bank_Account_Operated);
                $('#Bank_Name').val(data.Bank_Name);
                $('#Branch_Name').val(data.Branch_Name);
                $('#Branch_Id').val(data.Branch_Id);
                $('#Account_No').val(data.Account_No);
                $('#Account_Type').val(data.Account_Type);
                $('#MembershipFees').val(data.MembershipFees);
                $('#Income_Deposit_ForestOffice').val(data.Income_Deposit_ForestOffice);
                $('#Income_Sale_ForestProduce').val(data.Income_Sale_ForestProduce);
                $('#Other_Sources').val(data.Other_Sources);
                $('#Total_Income').val(data.Total_Income);
                $('#Benefits_Plant_Grass_Quantity').val(data.Benefits_Plant_Grass_Quantity);
                $('#Benefits_Plant_Grass_Amount').val(data.Benefits_Plant_Grass_Amount);
                $('#Minor_Forest_Produce_Quantity').val(data.Minor_Forest_Produce_Quantity);
                $('#Minor_Forest_Produce_Amount').val(data.Minor_Forest_Produce_Amount);
                $('#Other_ForestProduce_Quantity').val(data.Other_ForestProduce_Quantity);
                $('#Other_ForestProduce_Amount').val(data.Other_ForestProduce_Amount);
                $('#No_Of_Beneficiaries_LastThreeYear').val(data.No_Of_Beneficiaries_LastThreeYear);
                $('#No_Of_Beneficiaries_CurrentYear').val(data.No_Of_Beneficiaries_CurrentYear);
                $('#Efficient_Grades').val(data.Efficient_Grades);
                $('#No_Of_NGO_Working').val(data.No_Of_NGO_Working);
                $('#Self_Help_Group_Men').val(data.Self_Help_Group_Men);
                $('#Self_Help_Group_Women').val(data.Self_Help_Group_Women);
                $('#Self_Help_Group_Others').val(data.Self_Help_Group_Others);
                $('#Self_Help_Group_Total').val(data.Self_Help_Group_Total);
                $('#Overall_Remarks').val(data.Overall_Remarks);                                                
                $('#NewJfmc').show();
                $('html, body').animate({ scrollTop: $("#NewJfmc").offset().top - 120 }, 'slow');
            }
        });
    }

    function Block(Dist,Block) {
        $.ajax({
            type: 'POST',
            url: RootUrl + 'JFMCRegistration/getBlock', // we are calling json method
            dataType: 'json',
            data: { dist_code: Dist },
            success: function (states) {
                $.each(states, function (i, block) {
                    $("#Blk_Code").append('<option value="' + block.Value + '">' +
                     block.Text + '</option>');
                });
                $("#Blk_Code").val(Block);
            },
            error: function (ex) {
                alert('Failed to retrieve states.' + ex);
            }
        });
    }

    function GP(Dist, Block,Gp) {

        $.ajax({
            type: 'POST',
            url: RootUrl + 'JFMCRegistration/getGPs', // we are calling json method
            dataType: 'json',
            data: { dist_code: Dist, block: Block },
            success: function (states) {
                $.each(states, function (i, Gps) {
                    $("#Gp_Code").append('<option value="' + Gps.Value + '">' +
                     Gps.Text + '</option>');
                });
                $("#Gp_Code").val(Gp);
            },
            error: function (ex) {
                alert('Failed to retrieve states.' + ex);
            }
        });
    }

    function Village(Dist, Block, Gp,Vill) {      
        $.ajax({
            type: 'POST',
            url: RootUrl + 'JFMCRegistration/getVillage', // we are calling json method
            dataType: 'json',
            data: { District_code: Dist, Block_code: Block, GP_Code: Gp },
            success: function (states) {
                $.each(states, function (i, village) {
                    $("#Vill_Code").append('<option value="' + village.Value + '">' +
                     village.Text + '</option>');
                });
                $("#Vill_Code").val(Vill);
            },
            error: function (ex) {
                alert('Failed to retrieve states.' + ex);
            }
        });
    }

</script>
