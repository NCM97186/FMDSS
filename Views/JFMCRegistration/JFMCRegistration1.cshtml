            }
            else {
                var sum3 = $('#Income_Sale_ForestProduce').val();
            }
            if ($('#Other_Sources').val() == '') {
                var sum4 = 0;
            }
            else {
                var sum4 = $('#Other_Sources').val();
            }            
            $('#Total_Income').val(parseInt(sum1)+parseInt(sum2)+parseInt(sum3)+parseInt(sum4));
