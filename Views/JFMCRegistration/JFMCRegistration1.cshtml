    $(function (e) {

        $('#Minutes_of_Meeting').change(function (e) {
            if ($('#Minutes_of_Meeting option:selected').val() == "Yes") {
                $('#UploadMom').show();
            }
            else {
                $('#UploadMom').hide();
            }
        });


        $('#Dist_Code').change(function (e) {
            $("#Blk_Code").empty();
            $("#Gp_Code").empty();
            $("#Vill_Code").empty();
            $("#Blk_Code").append('<option value="' + '' + '">' +
                    '--Select--' + '</option>');
            $.ajax({
                type: 'POST',
                url: RootUrl + 'JFMCRegistration/getBlock', // we are calling json method
                dataType: 'json',
                data: { dist_code: $("#Dist_Code").val() },
                success: function (states) {
                    $.each(states, function (i, block) {
                        $("#Blk_Code").append('<option value="' + block.Value + '">' +
                         block.Text + '</option>');
                    });
                },
                error: function (ex) {
                    alert('Failed to retrieve states.' + ex);
                }
            });
        });

        $('#Blk_Code').change(function (e) {
            $("#Vill_Code").empty();
            $("#Gp_Code").empty();
            $("#Gp_Code").append('<option value="' + '' + '">' +
                   '--Select--' + '</option>');
            $.ajax({
                type: 'POST',
                url: RootUrl + 'JFMCRegistration/getGPs', // we are calling json method
                dataType: 'json',
                data: { dist_code: $("#Dist_Code").val(), block: $("#Blk_Code").val() },
                success: function (states) {
                    $.each(states, function (i, Gps) {
                        $("#Gp_Code").append('<option value="' + Gps.Value + '">' +
                         Gps.Text + '</option>');
                    });
                },
                error: function (ex) {
                    alert('Failed to retrieve states.' + ex);
                }
            });
        });

        $('#Gp_Code').change(function (e) {
            $("#Vill_Code").empty();
            $("#Vill_Code").append('<option value="' + '' + '">' +
                  '--Select--' + '</option>');
            $.ajax({
                type: 'POST',
                url: RootUrl + 'JFMCRegistration/getVillage', // we are calling json method
                dataType: 'json',
                data: { District_code: $("#Dist_Code").val(), Block_code: $("#Blk_Code").val(), GP_Code: $("#Gp_Code").val() },
                success: function (states) {
                    $.each(states, function (i, village) {
                        $("#Vill_Code").append('<option value="' + village.Value + '">' +
                         village.Text + '</option>');
                    });
                },
                error: function (ex) {
                    alert('Failed to retrieve states.' + ex);
                }
            });
        });


        $('#Bank_Account_Operated').change(function (e) {

            if ($('#Bank_Account_Operated option:selected').val() == "No") {

                $('#Bank_Name,#Branch_Name,#Branch_Id,#Account_No,#Account_Type').val('');
                $('.bank').hide();
            }
            if ($('#Bank_Account_Operated option:selected').val() == "Yes") {
                $('.bank').show();
            }

        });

        $('.sum').bind('change blur',function (e) {                     
            if ($('#MembershipFees').val() == '') {
                var sum1 = 0;
            }
            else {
                var sum1 = $('#MembershipFees').val();
            }
