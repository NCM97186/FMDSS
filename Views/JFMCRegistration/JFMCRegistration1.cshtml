            //    $('#Longitude').focus();
            //    return false;
            //}
           else if ($('#Bank_Account_Operated option:selected').val() == "Yes" && $('#Bank_Name').val()=='') {
                alert('Enter bank name');
                $('#Bank_Name').focus();
                e.preventDefault();
            }
           else if ($('#Bank_Account_Operated option:selected').val() == "Yes" && $('#Branch_Name').val() == '') {
                alert('Enter branch name');
                $('#Branch_Name').focus();
                e.preventDefault();
            }
           else if ($('#Bank_Account_Operated option:selected').val() == "Yes" && $('#Branch_Id').val() == '') {
                alert('Enter branch id');
                $('#Branch_Id').focus();
                e.preventDefault();
            }
           else if ($('#Bank_Account_Operated option:selected').val() == "Yes" && $('#Account_No').val() == '') {
                alert('Enter account no');
                $('#Account_No').focus();
                e.preventDefault();
