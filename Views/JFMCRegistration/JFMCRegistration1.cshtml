<script type="text/javascript">


    $(document).ready(function () {
        

        if ($('option:selected', $('#Vill_Code')).index() == 0) {
            
            $("#LinkGetLatLong").css("display", "none");
            

        }
        else {
            $("#LinkGetLatLong").css("display", "block");
