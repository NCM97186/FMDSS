    function LoadProduceName() {
        try {
            $.ajax({
                type: 'POST',
                headers: {
                    __RequestVerificationToken: $('[name= "__RequestVerificationToken"]').val(),
                },
                url: RootURl + 'TransitPermitCitizen/ProduceList',
                dataType: 'json',
                data: {},
                success: function (data) {
                    if (data != null) {
                        for (var i = 0; i < data.list1.length; i++) {
                            var count = 0;
                            count = count + i;
                            var trRow = "trRow" + count;
                            var bardata = "<tr class='trRow'><td><input id=" + trRow + " type='checkbox' class = 'SelectTag' style=cursor:pointer onclick=updateTextArea() /></td><td style='display:none'>" + data.list1[i].PRODUCE_ID + "</td><td>" + data.list1[i].PRODUCE_NAME + "</td></tr>";
                            $("#tbobyProduce").append(bardata);
                        }
                    }
                },
                error: function (ex) {
                    ConfirmBox("Exception", ex.Message)
                }
            });
        }
        catch (err) { alert(err.message); }
    }

    function updateTextArea() {
        try {
            var allVals = [];
            var allValIds = [];
            $('.trRow :checked').each(function () {

                var $itemId = $(this).closest("tr").find('td:eq(1)');
                var j = 0;
                $.each($itemId, function (key, value) {
                    if (j == 0) {
                        allValIds.push($(value).text());
                    }
                    j++;
                });
                var $item = $(this).closest("tr").find('td:eq(2)');
                var i = 0;
                $.each($item, function (key, value) {

                    if (i == 0) {

                        allVals.push($(value).text());
                    }
                    i++;
                });
            });
            var a = allVals.toString().split(',').join('\n');
            $('#PRODUCE_NAME').val(a);
            var b = allValIds.toString().split(',');
            $('#PRODUCE_ID').val(b);

        }
        catch (err) { alert(err.message); }
    };

    $(document).ready(function () {



        $('#FROM_STATE').change(function (e) {
            $("#FROM_DISTRICT").empty();
            $("#FROM_DISTRICT").append('<option value="">--Select--</option>');
            $.ajax({
                type: 'POST',
                headers: {
                    __RequestVerificationToken: $('[name= "__RequestVerificationToken"]').val(),
                },
                url: RootURl + 'Elephant/DistrictData', // we are calling json method
                dataType: 'json',
                data: { StateId: $('#FROM_STATE').val() },
                success: function (data) {
                    $.each(data, function (i, items) {
                        $("#FROM_DISTRICT").append('<option value=' + items.Value + '>' + items.Text + '</option>');
                    });
                },
                error: function (ex) {
                    alert('Failed to retrieve states.' + ex);
                }

            });
            return false;
        });



        $('#TO_STATE').change(function (e) {
            $("#TO_DISTRICT").empty();
