                success: function (data) {
                    $.each(data, function (i, items) {
                        $("#TO_DISTRICT").append('<option value=' + items.Value + '>' +
                         items.Text + '</option>');
                    });
                },
                error: function (ex) {
                    alert('Failed to retrieve states.' + ex);
                }

            });
            return false;
        });

