        //encription

        var key = CryptoJS.enc.Utf8.parse('8080808080808080');
        var iv = CryptoJS.enc.Utf8.parse('8080808080808080');


        var encryptedAdhar = CryptoJS.AES.encrypt(CryptoJS.enc.Utf8.parse(AadharOrBhamashaNumber), key,
            {
                keySize: 128 / 8,
                iv: iv,
                mode: CryptoJS.mode.CBC,
                padding: CryptoJS.pad.Pkcs7
            });

        //alert(encryptedAdhar);
        $('#hdAadharOrBhamashaNumber').val(encryptedAdhar);


        //end



        if (AadharOrBhamashatype == 1)//1 means bhamasha and 0 means AAdhar
