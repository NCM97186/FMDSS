        function EditProject(ProjectId) {
            var d = ProjectId.split(',');

            $("#Dropmodel").empty();
            $("#Dropdivision").empty();
            $.ajax({
                type: 'POST',
                url: RootUrl + 'FdmAddProject/GetModel', // we are calling json method
                dataType: 'json',
                data: { SchemeId: d[1] },
                success: function (data) {
                    for (var i = 0; i < data.list1.length; i++) {
                        $("#Dropmodel").append('<option value="' + data.list1[i].Value + '">' + data.list1[i].Text + '</option>');
                    }
                    for (var i = 0; i < data.list2.length; i++) {
                        $("#Dropdivision").append('<option value="' + data.list2[i].Value + '">' + data.list2[i].Text + '</option>');
                    }
                    $('#SchemeAmount').val(data.list3);
                    $("#SchemeAmount").attr('disabled', 'disabled');
                },
                error: function (ex) {
                    alert('Failed to retrieve states.' + ex);
                }
            });





            $('#hdn_id').val(d[0]);
            var ProjectId = {
                ProjectId: $.trim(d[0])
            }
            $.ajax({
                type: 'POST',
                url: RootUrl + 'FdmAddProject/Edit',
                contentType: 'application/json; charset=utf-8',
                data: JSON.stringify(ProjectId),
                success: function (data) {
                        
                        var model = data.Model_Code;
                        var dcode = model.split(',');
                        if (model.length > 0) {
                            $("#Dropmodel").val(dcode);
                            //  $("#Dropmodel").attr('disabled', 'disabled');
                            //$('#Dropmodel').multiselect("refresh");
                        }
                        var division = data.Dist_Code;

                        var divdcode = division.split(',');
                        if (division.length > 0) {
                            $('#Dropdivision').val(divdcode);
                            //  $('#Dropdivision').multiselect("refresh");
                            // $("#Dropdivision").attr('disabled', 'disabled');
                        }

                        $('#ProjectName').val(data.Project_Name);
                        $('#Startdate').val(data.StartDate1);
                        $('#Enddate').val(data.EndDate1);
                        $('#ArRollout').val(data.AreaofRolloutinSQKM);
                        $('#Dropscheme option').filter(function () { return $(this).val() == data.Scheme_Id }).attr('selected', true);
                        $('#RefNodoc').val(data.ReferenceNo);
                        var filename = data.RefDocument;
                        var str = filename.split('/');
                        if (data.RefDocument != '') {
                            $('#viewimgREF').show();
                            $('#viewimgREF').attr('href', '../FixedLandDocument/' + str[2]);
                            $('#viewimgREF').attr('target', '_blank');
                        }
                        var filenamedpr = data.DPRDocument;
                        var strdpr = filenamedpr.split('/');
                        if (data.DPRDocument != '') {
                            $('#viewimgDPR').show();
                            $('#viewimgDPR').attr('href', '../FixedLandDocument/' + strdpr[2]);
                            $('#viewimgDPR').attr('target', '_blank');
                        }
                        $('#SchemeAmount').val(data.SchemeBudget);
                        $("#SchemeAmount").attr('disabled', 'disabled');
                        $('#EBudget').val(data.EstimatedBudget);
                        $('#hd_EnteryType').val('Edit');
                        $('#btn_submit1').text('Update');
                   
                },
                error: function (ex) {
                    alert('Failed to retrieve states.' + ex);
                }
            });
        }
        function ViewProject(ProjectId) {
            var d = ProjectId.split(',');
            $("#Dropmodel").empty();
            $("#Dropdivision").empty();
            $.ajax({
                type: 'POST',
                url: RootUrl + 'FdmAddProject/GetModel', // we are calling json method
                dataType: 'json',
                data: { SchemeId: d[1] },
                success: function (data) {
                    for (var i = 0; i < data.list1.length; i++) {
                        $("#Dropmodel").append('<option value="' + data.list1[i].Value + '">' + data.list1[i].Text + '</option>');
                    }
                    for (var i = 0; i < data.list2.length; i++) {
                        $("#Dropdivision").append('<option value="' + data.list2[i].Value + '">' + data.list2[i].Text + '</option>');
                    }
                    $('#SchemeAmount').val(data.list3);
                    $("#SchemeAmount").attr('disabled', 'disabled');
                },
                error: function (ex) {
                    alert('Failed to retrieve states.' + ex);
                }
            });

            $('#hdn_id').val(d[0]);
            var ProjectId = {
                ProjectId: $.trim(d[0])
            }
            $.ajax({
                type: 'POST',
                url: RootUrl + 'FdmAddProject/Edit',
                contentType: 'application/json; charset=utf-8',
                data: JSON.stringify(ProjectId),
                success: function (data) {
                    $('#ProjectName').val(data.Project_Name);
                    $('#Startdate').val(data.StartDate1);
                    $('#Enddate').val(data.EndDate1);
                    $('#ArRollout').val(data.AreaofRolloutinSQKM);
                    $('#Dropscheme option').filter(function () { return $(this).val() == data.Scheme_Id }).attr('selected', true);
                    $('#RefNodoc').val(data.ReferenceNo);
                    var filename = data.RefDocument;
                    var str = filename.split('/');
                    if (data.RefDocument != '') {
                        $('#viewimgREF').show();
                        $('#viewimgREF').attr('href', '../FixedLandDocument/' + str[2]);
                        $('#viewimgREF').attr('target', '_blank');
                    }
                    var filenamedpr = data.DPRDocument;
                    var strdpr = filenamedpr.split('/');
                    if (data.DPRDocument != '') {
                        $('#viewimgDPR').show();
                        $('#viewimgDPR').attr('href', '../FixedLandDocument/' + strdpr[2]);
                        $('#viewimgDPR').attr('target', '_blank');
                    }
                    $('#SchemeAmount').val(data.SchemeBudget);
                    $("#SchemeAmount").attr('disabled', 'disabled');
                    $('#EBudget').val(data.EstimatedBudget);
                    $('#hd_EnteryType').val('Edit');
                    $('#Dropscheme,#ProjectName,#Startdate,#Enddate,#RefNodoc,#ArRollout,#EBudget,#Upload_DPR,#Upload_Button,#Dropmodel,#Dropdivision').attr('disabled', 'disabled');
                    $('#btn_submit1,#btnReset').hide();
                    $('#btnCancel').text('close');
                },
                error: function (ex) {
                    alert('Failed to retrieve states.' + ex);
                }
            });

        }
    </script>

