    function CallApplicationNo(id) {


        $.ajax({
            type: 'GET',
            url: RootUrl + 'MISCitizen/GetNOCApplicationDetails', // we are calling json method
            dataType: 'html',
            data: { ids: id },
            success: function (data) {
                $('#modelPlace').html(data);
            },
            error: function (ex) {
                alert(ex.error);
            }
        });

    };



</script>

