@model FMDSS.Models.Encroachment.DomainModel.Tbl_Encroacher_Details

<script>
    $('footer.foot-inn').hide();
</script>

<div class="col-lg-6">
    <div class="form-group">
        <label>Encroacher Name: <span class="mandatory">*</span></label>
        @Html.TextBoxFor(x => x.Encroacher_Name, new { @class = "form-control", @id = "Encroacher_Name", name = "Encroacher_Name" })
        <div id="errJFMC" class="alert-danger1">@Html.ValidationMessageFor(x => x.Encroacher_Name)</div>
    </div>
</div>


<div class="col-lg-6">
    <div class="form-group">
        <label>Encroacher Address: <span class="mandatory">*</span></label>
        @Html.TextBoxFor(x => x.Encroacher_Address, new { @class = "form-control", @id = "Encroacher_Address", name = "Encroacher_Address" })
        <div id="errJFMC" class="alert-danger1">@Html.ValidationMessageFor(x => x.Encroacher_Address)</div>
    </div>
</div>

