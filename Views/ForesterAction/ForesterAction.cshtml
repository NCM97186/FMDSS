<link href="~/css/bootstrap-timepicker.min.css" rel="stylesheet" />
<script src="~/js/bootstrap-timepicker.min.js"></script>
<script type="text/javascript">

    $('.timepicker1').timepicker();
    $(document).ready(function () {

        var d = new Date($('#hdCurrentdate').val());
        d.setDate(d.getDate());
        var day = d.getDate();
        var month = d.getMonth() + 1;
        var year = d.getFullYear();
        if (day < 10) {
            day = "0" + day;
        }
        if (month < 10) {
            month = "0" + month;
        }
        var tdate = day + "/" + month + "/" + year;
        $(".datefield").datepicker({ minDate: tdate, dateFormat: 'dd/mm/yy', changeYear: true, onClose: function (dateText, inst) { $("[id $=auto]").focus(); } });
        //  var currentDate = new Date();
        $("#txt_DateOfOffense").datepicker("setDate", tdate);

    });
</script>
