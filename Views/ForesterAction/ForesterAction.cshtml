            }
            //if (i == 1) {
            //    TableName = $(value).text();
            //}
            i++;
        });

        var tblinfo = {
            RequestId: $.trim(RequestId),
            TableName: $.trim("Tbl_Citizen_TransitPermit")
        }
        $.ajax({
            type: 'POST',
            url: RootUrl + 'ForesterAction/ViewDetails',
            contentType: 'application/json; charset=utf-8',
            data: JSON.stringify(tblinfo),
            success: function (data) {
                if (tblinfo.TableName == 'Tbl_Citizen_TransitPermit') {
                    $('#TransitPermit').text('');
                    $('#TransitPermit').append(data);
                }

            },
            error: function (ex) {
                alert('Failed to retrieve states Rajveer.' + ex);
            }
