<script>
    var RootUrlLayout = '@Url.Content("~/")';
    function ShowKPIs(input) {
        var resultData = null;
        $.ajax({
            type: 'POST',
            url: RootUrl + 'KPIs/ShowKPIs', // we are calling json method
            dataType: 'json',
            data: { ActionType: input },
            success: function (result) {
                resultData = JSON.stringify(result);
                if (input == 'VFMC') { input = 'VFMC'; }
                document.getElementById("ssoid").value = '@Session["SSOID"]';
                document.getElementById("caption").value = 'No. of ' + input;
                document.getElementById("themeName").value = input;
                document.getElementById("themeData").value = resultData;
                document.getElementById("kpiForm").submit();
            },
            error: function (ex) {
                alert('Failed to retrieve states.' + ex);
            }
        });



        return false;
    }

    function PostPublishNotice(noticeId) {
        $("#lbpl_header").text('Publish Notice');
        var tblinfo = {
            NoticeId: $.trim(noticeId)
        }

        $.ajax({
            type: 'POST',
            url: RootUrlLayout + 'ManageNotice/PublishNotice',
            contentType: 'application/json; charset=utf-8',
            data: JSON.stringify(tblinfo),
            success: function (data) {
                $("#divPublish").html(data.list1);
                $("#Amendments").html(data.list2);
            },
            error: function (ex) {
                alert('Failed to retrieve states.' + ex);
            }
        });
    }

</script>











