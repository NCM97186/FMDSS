<script>


    function ShowKPIs(input) {
        var resultData = null;
 
        $.ajax({
            type: 'POST',
            url: RootUrl + 'KPIs/ShowKPIs', // we are calling json method
            dataType: 'json',
            data: { ActionType: input },
            success: function (result) {
                
                    resultData = JSON.stringify(result);
                
                document.getElementById("ssoid").value = '@Session["SSOID"]';                               
                document.getElementById("themeData").value = resultData;
                document.getElementById("kpiForm").submit();
                
            },
            error: function (ex) {
                alert('Failed to retrieve states.' + ex);
            }
        });


       
        return false;
    }

</script>
<form name="kpiForm" id="kpiForm" target="_blank" action="http://10.68.128.101/fmdsstest/FMDSSGIS/kpi/theme.aspx" method="post">
    <input type="hidden" id="ssoid" name="ssoid" />
    <input type="hidden" id="portalid" name="portalid" value="portal123" />
    <input type="hidden" id="caption" name="caption" value="No. of VFPMCs" />
    <input type="hidden" id="themeName" name="themeName" value="VFPMC" />
    <input type="hidden" id="adminType" name="adminType" value="District" />
    <input type="hidden" id="themeData" name="themeData" value="NA" />
</form>
