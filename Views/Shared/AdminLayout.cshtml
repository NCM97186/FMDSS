                $("#Permissions").append('<option value=0>--Please Select Permission--</option>');
                $.ajax({
                    type: 'POST',
                    url: RootURl + 'SystemAdmin/GetPermission',
                    dataType: 'json',
                    data: { moduleId: $moduleId, ServiceId: $ServiceId },
                    success: function (states) {

                        $.each(states, function (i, items) {
                            $("#Permissions").append('<option value="' + items.Value + '">' +
                             items.Text + '</option>');
                        });
                    },
                    error: function (ex) {
                        alert('Failed to retrieve states.' + ex);
                    }
                });
