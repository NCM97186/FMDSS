@model IEnumerable<FMDSS.Models.Admin.EnchorsmentReport>

<div class="modal-dialog">
    <div class="modal-content">
        <div class="row">
            <div class="col-lg-12">
                <div class="panel panel-default">
                    <div class="panel-body">
                        <table class="table table-striped table-bordered table-hover">
                            <thead>
                                <tr>
                                    <th width="10%">SNo</th>
                                    @*<th width="10%">Enchorsment_Code</th>*@
                                    <th width="10%">InvestigationOfficer</th>
                                    <th width="10%">Decision Date</th>
                                    <th width="10%">Next Decision Place</th>
                                    <th width="10%">Decision Taken</th>
                                    <th width="10%">Next Date</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>@item.SNo</td>
                                        @*<td>@item.Enchorsment_Code</td>*@
                                        <td>@item.InvestigationOfficer</td>
                                        <td>@item.Decision_Date</td>
                                        <td>@item.Next_Decision_Place</td>
                                        <td>@item.Decision_Taken</td>
                                        <td>@item.Next_Date</td>
                                    </tr>
                                }
                            </tbody>
                        </table>

                    </div>

                </div>

            </div>
            <!-- /.modal-body -->
        </div>
    </div>
</div>
@Html.Hidden("hdnRootURL", Url.Content("~/"))
<div class="modal fade in" data-backdrop="static" id="myModalDashboard" tabindex="2" role="dialog" aria-hidden="false"></div>


