@{
    /**/

    ViewBag.Title = "OnlineUser";
    Layout = "~/Views/Shared/AdminLayout.cshtml";

    //Layout = Convert.ToString(HttpContext.Current.Session["CurrentLayout"].ToString()); // "~/Views/Shared/_SuperAdmin.cshtml";
}
 




<script type="text/javascript" src="~/js/jquery.min.js"></script>


<script type="text/javascript">
    var RootUrl = '@Url.Content("~/")';

    $(document).ready(function () {
        fillTable();
        //$('#tbltemp').DataTable({
        //    responsive: true
        //});

    });

        function fillTable() {
        $.ajax({
            type: 'GET',
            url: RootUrl + 'SystemAdmin/GetOnlineUser', // we are calling json method
            contentType: 'application/json; charset=utf-8',
            dataType: 'json',
            success: function (data) {
                $.each(data, function (i, items) {

                    //alert(items.StartTime);
                    $("#tbltemp").append('<tr><td>' + items.Index + '</td><td>' + items.StartTime + '</td><td>' + items.ENDTIME + '</td><td>' + items.Name + '</td><td>' + items.Mobile + '</td><td>' + items.EmailId + '</td><td>' + items.IPAddress + '</td><td>' + items.OfficeBoundary + '</td><td>' + items.OfficeName + '</td></tr>');
                });

            },
            error: function (req, err) {
                debugger;
                console.log('my message: ' + err);
            }

        });

    }





</script>

<div id="page-wrapper-inn">
    <div class="col-lg-12 pad">
        <h1 class="page-header">FMDSS Online Department User(s)</h1>
    </div>
    <div class="row">
        @*<div class="col-lg-4 pull-right" id="place">
