@model FMDSS.Models.CitizenService.PermissionServices.NocData
@{
    Layout = "~/Views/Shared/_SWCSLayout.cshtml";
    string rootPath = "http://10.68.128.179";
}

<div class="main_investor_registration">
    <div class="line">
        <h2 class="section-title" style="color: #fff;">NOC Detail</h2>
    </div>
    <div class="container-fluid investor_registration_box">
        <div id="divPrint" class="tbl-scroll divPrintone" style="height:auto;">
            <div class="col-md-10" style="padding-right:0">
                <h4 class="modal-title" id="myModalLabel">
                    Request Details
                    @*<button type="button" name="btnprint" id="btnprint" onclick="Print();" class="btn btn-default pull-right">
                            <i class="fa fa-print"></i> Print
                        </button>*@
                </h4>
            </div>

            @using (Html.BeginForm("BackToSWCS", "NOC", FormMethod.Post, new { enctype = "multipart/form-data", data_behavior = "confirm" }))
            {
            <input type="hidden" id="UserDetail" name="UserDetail" value="@ViewBag.UserDetail" />
            <button type="submit" class="btn-success" style="float:right;" value="Back To SWCS"> Back To SWCS</button>
            }
            <table class="table table-striped table-bordered table-hover">
                <tbody id="tbReqDetail">
                    @Html.Raw(ViewBag.ReqData.Replace("\r\n", "<br />"))
                </tbody>
            </table>
        </div>
    </div>
</div>

