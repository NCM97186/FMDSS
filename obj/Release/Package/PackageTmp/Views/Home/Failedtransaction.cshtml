        <script src="~/js/dashboard/dataTables.bootstrap.min.js"></script>

        <script>
            var RootUrl = '@Url.Content("~/")';

            $(document).ready(function () { 
                var currentYear = (new Date).getFullYear();

                var Range = '2008:' + (currentYear + 1)
                $("#VisitDate").datepicker({ yearRange: Range, dateFormat: 'dd/mm/yy', changeMonth: true, changeYear: true, onClose: function (dateText, inst) { $("[id$=auto]").focus(); } });
                var currentDate = new Date();

                $(document).on('keypress', '#VisitDate', function (event) {

                    event.preventDefault();
                    return false;

                });

                $('#VisitDate').keydown(function (event)
                { return cancelBackspace(event) });
                function cancelBackspace(event) {
                    if (event.keyCode == 8 || event.keyCode == 46) {
                        return false;
                    }
                }

                // date validation ========================================================================





                

            });


        </script>





