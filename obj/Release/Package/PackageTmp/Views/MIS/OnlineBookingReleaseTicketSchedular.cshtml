@model IEnumerable<FMDSS.Models.MIS.OnlineBookingReleaseTicketSchedularModel>
@{
    Layout = Convert.ToString(HttpContext.Current.Session["CurrentLayout"].ToString());
    ViewBag.Title = "OnlineBookingReleaseTicketSchedular";
}

<style>
    #tbl {
        overflow-x: auto;
        overflow-y: auto;
        padding: 15px;
        width: 100%;
        height: 400px;
    }
</style>


<div class="row">
    <div class="col-lg-9 pad">
        <h1 class="page-header"><span>Report : Release Booking Tickets</span></h1>
    </div>


    <div class="col-lg-9">

        <div class="panel panel-default">
            <div class="panel-body">
                <div class="dataTable_wrapper">
                    <div id="tbl" class="table-responsive">

                        <table class="table table-striped table-bordered table-hover table-responsive gridtable">

                            <thead>
                                <tr>
                                    <th>#</th>
                                    <th>RequestID</th>
                                    <th>Ticket ID</th>
                                    <th>Transation Status</th>
                                    <th>Enter Date </th>
                                    <th>Current Date</th>
                                </tr>
                            </thead>
                            <tbody>
                                @{int index = 1; }
                                @foreach (var item in Model.Select((value, i) => new { i, value }))
                                {
                                    <tr>
                                        <td>@index</td>
                                        <td>@Html.DisplayFor(model => item.value.RequestId)</td>
                                        <td>@Html.DisplayFor(model => item.value.TicketId)</td>
                                        <td>@Html.DisplayFor(model => item.value.Trn_Status)</td>
                                        <td>@Html.DisplayFor(model => item.value.TicketBookingEnterOn)</td>
                                        <td>@Html.DisplayFor(model => item.value.CreatedDate)</td>
                                       
                                    </tr>
                                                index++;
                                            }

                            </tbody>
                        </table>
                    </div>

                </div>
            </div>

        </div>




    </div>
</div>


<script src="~/Scripts/jquery.blockUI.js"></script>
<script>

    $(document).ajaxStart($.blockUI).ajaxStop($.unblockUI);
</script>

<script type="text/javascript">
    $.noConflict(true);
</script>

<script type="text/javascript" src="~/js/jquery.min.js"></script>
<script src="~/Scripts/jquery-ui.js"></script>

