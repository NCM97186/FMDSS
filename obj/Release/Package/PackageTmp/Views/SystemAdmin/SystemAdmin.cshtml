            $('#btnGo').click(function (e) {
                if (($('#Permissions').val() != 0) && ($('#SubPermissions').val() != 0)) {
                    $("#DetailEmployee").empty();
                    debugger;
                    $.ajax({
                        type: 'POST',
                        url: RootURl + 'SystemAdmin/getForestEmployees',
                        dataType: 'json',
                        data: { moduleId: $moduleId, ServiceId: $ServiceId, PermissionId: $('#Permissions').val(), SubPermissionId: $('#SubPermissions').val(), officeCode: $("#ForestOffices").val() },
                        success: function (data) {
                            var IsReviewer = ''; var IsApprover = '';
                            $.each(data, function (i, items) {
                                IsReviewer = ''; IsApprover = '';
                                if (items.IsReviewer == true)
                                    IsReviewer = 'Checked'
                                if (items.IsApprover == true)
                                    IsApprover = 'Checked'

                                var bardata = "<tr class='rowid'><td >" + items.EMPNAME + "</td><td>" + items.EMPSSOID + "</td><td>" + items.EMPDESIGNATION + "</td><td align='center'><input id='chkReview" + i + "' type='checkbox' " + IsReviewer + " /> </td><td align='center'><input  id='ChkApprover" + i + "' type='checkbox' " + IsApprover + " /></td><td><input class='btn btn-primary' id='btnSavePermission" + i + "' type='Submit' value='Save' onclick='SaveUserPermission(" + i + "," + items.UserId + "," + items.Designation + ");'/></td></tr>";
                                $("#DetailEmployee").append(bardata);
                            })

                            $('#width-contrl').DataTable({
                                paging: true,
                                destroy: true
                            });
                        },
                        error: function (ex) {
                            alert('Failed to retrieve states.' + ex);
                            return false;
                        }
                    });
                }
                else { alert('Kindly select permission type!'); return false; }
            })

            function SaveUserPermission(object, object2, object3) {
                var moduleId = $moduleId;
                var serviceTypeId = $ServiceId;
                var permissionId = $("#Permissions").val();
                var subPermissionId = $("#SubPermissions").val();
                var userId = object2;
                var roleId = 'FORESTER';
                var designation = object3;
                var office = $("#ForestOffices").val();
                var department = 2;
                var jurisdiction = 2;
                var pecuniaryLimit = 2;
                var isApprover = $("#ChkApprover" + object).is(':checked') ? 1 : 0;
                var isReviewer = $("#chkReview" + object).is(':checked') ? 1 : 0;
                //var updatedBy = HttpContext.Current.Session["User"];

                var UserApproval = {
                    ModuleId: moduleId,
                    ServiceTypeId: serviceTypeId,
                    PermissionId: permissionId,
                    SubPermissionId: subPermissionId,
                    UserId: userId,
                    RoleId: roleId,
                    Designation: designation,
                    Office: office,
                    Department: department,
                    Jurisdiction: jurisdiction,
                    PecuniaryLimit: pecuniaryLimit,
                    IsReviewer: isReviewer,
                    IsApprover: isApprover
                    // UpdatedBy:updatedBy
                };

                $.ajax({
                    type: 'POST',
                    contentType: 'application/json; charset=utf-8',
                    data: JSON.stringify(UserApproval),
                    url: RootURl + 'SystemAdmin/SaveUserApproval',
                    success: function (data) {
                        if (data == 'failed') {
                            $('#divError').html(data);
                            $('#divError').css("display", "block");
                        }
                        else {
                            $('#divSuccess').html(data);
                            $('#divSuccess').css("display", "block");
                        }
                    },
                    traditional: true,
                    // error: function (data) { console.log(data) }
                });
            };

        </script>
