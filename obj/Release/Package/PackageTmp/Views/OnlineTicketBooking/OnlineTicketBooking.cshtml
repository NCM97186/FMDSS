@{
    ViewBag.Title = "OnlineTicketBooking";
    Layout = Convert.ToString(HttpContext.Current.Session["CurrentLayout"].ToString()); // "~/Views/Shared/_Layout.cshtml";
}
@model IEnumerable<FMDSS.Models.OnlineBooking.TicketBooking>

<script src="~/js/jquery.min.js"></script>
<script type="text/javascript">
    var RootURl = '@Url.Content("~/")';
    $(function () {
        $(".datefield").datepicker({ minDate: new Date(), dateFormat: 'dd/mm/yy', changeYear: true, onClose: function (dateText, inst) { $("[id$=auto]").focus(); } });
    });

    $(document).ready(function (e) {
        
        $("#ddl_Category").bind("change", function () {
            var category = $("#ddl_Category").val();
            document.getElementById("errorType").style.display = 'none';
             
            $('#ddl_Districts').empty();
            var values = {
                Category: category
            };

            $.ajax({
                type: 'POST',
                contentType: 'application/json; charset=utf-8',
                data: JSON.stringify(values),
                url: RootURl + 'OnlineTicketBooking/DistrictbyCategory',
                success: function (data) {
                    $.each(data, function (i, items) {
                        $("#ddl_Districts").append('<option value="' + items.Value + '">' + items.Text + '</option>');
                    });
                },
                traditional: true,
                error: function (data) { console.log(data) }
            });
        });


        $("#ddl_Districts").bind("change", function () {

            var districtID = $("#ddl_Districts").val();
            $('#ddl_place').empty();
            var values = {
                DistrictID: districtID
            };

            $.ajax({
                type: 'POST',
                contentType: 'application/json; charset=utf-8',
                data: JSON.stringify(values),
                url: RootURl + 'OnlineTicketBooking/PlaceByDistrict',
                success: function (data) {
                    $.each(data, function (i, items) {
                        $("#ddl_place").append('<option value="' + items.Value + '">' + items.Text + '</option>');
                    });
                },
                traditional: true,
                error: function (data) { console.log(data) }
            });
            document.getElementById("errorDis").style.display = 'none';
        });

        $("#ddl_place").bind("change", function () {
            $('#ddl_Shift').empty();

            $('#lbl_IndAdult').html('');
            $('#lbl_IndChild').html('');
            $('#lbl_ForAdultfee').html('');
            $('#lbl_ForChildfee').html('');
            $('#lbl_Student').html('');
            $('#lbl_guidefee').html('');
            $('#lbl_camerafee').html('');
            $('#lbl_singleOccupancyfee').html('');
            $('#lbl_doubleOccupancy').html('');
            $('#lbl_safarifee').html('');
            $('#txt_discount').val('');
            $('#txt_taxRate').val('');

            var districtID = $("#ddl_Districts").val();
            var placeID = $("#ddl_place").val();
            var values = {
                DistrictID: districtID,
                PlaceID: placeID
            };

            $.ajax({
                type: 'POST',
                contentType: 'application/json; charset=utf-8',
                data: JSON.stringify(values),
                url: RootURl + 'OnlineTicketBooking/feesByDistrictPlace',
                success: function (data) {
                    $.each(data, function (i, items) {
                        $('#lbl_IndAdult').text("Fees per Adult: " + items.IndianAdultFees);
                        $('#lbl_IndChild').text("Fees per child: " + items.IndianChildFees);
                        $('#lbl_ForAdultfee').text("Fees per adult: " + items.ForeignerAdultFees);
                        $('#lbl_ForChildfee').text("Fees per child: " + items.ForeignerChildFees);
                        $('#lbl_Student').text("Fees per Student: " + items.StudentFees);
                        $('#lbl_guidefee').text("Fees per guide: " + items.GuideFees);
                        $('#lbl_camerafee').text("Fees per camera: " + items.CameraFees);
                        $('#lbl_singleOccupancyfee').text("Fees per room: " + items.SingleOccupancyFees);
                        $('#lbl_doubleOccupancy').text("Fees per room: " + items.DoubleOccupancyFees);
                        $('#lbl_safarifee').text("Fees per member: " + items.SafariFees);
                        $('#txt_discount').val(items.DiscountPercentage);
                        $('#txt_taxRate').val(items.TaxRate);
                       
                        if (items.FullDayShift == '1')
                        {
                            $("#ddl_Shift").append('<option value="' + 0 + '">' + "--Select--" + '</option>');
                            $("#ddl_Shift").append('<option value="' + 3 + '">' + "Full Day" + '</option>');
                        }
                        else if(items.FullDayShift == '0')
                        {
                            $("#ddl_Shift").append('<option value="' + 0 + '">' + "--Select--" + '</option>');
                            $("#ddl_Shift").append('<option value="' + 1 + '">' + "Morning" + '</option>');
                            $("#ddl_Shift").append('<option value="' + 2 + '">' + "Evening" + '</option>');
                        }
                        else
                        {                           
                            $("#ddl_Shift").append('<option value="' + 0 + '">' + "--Select--" + '</option>');
                        }
                    });
                },
                traditional: true,
                error: function (data) { console.log(data) }
            });
            document.getElementById("errorPlace").style.display = 'none';
        });
     
      

        $("#ddl_Shift").change(function () {
            var arrivalDate = $("#txt_dateofarrival").val();
            if (arrivalDate) {
                var placeID = $("#ddl_place").val();
                var shiftTime = $("#ddl_Shift").val();
                //  alert(placeID + ' ' + arrivalDate + ' ' + shiftTime)
                var values = {
                    PlaceID: placeID,
                    ArrivalDate: arrivalDate,
                    ShiftTime: shiftTime
                };

                $.ajax({
                    type: 'POST',
                    contentType: 'application/json; charset=utf-8',
                    data: JSON.stringify(values),
                    url: RootURl + 'OnlineTicketBooking/CheckTicketAvailability',
                    success: function (data) {
                        $('#lbl_NumberofMembersavailable').text("Currently available seats: " + data);
                    },
                    traditional: true,
                    error: function (data) { console.log(data) }
                });
            }
            document.getElementById("errorShift").style.display = 'none';
        });
        $("#txt_dateofarrival").change(function () {
            var shiftTime = $("#ddl_Shift").val();
            if (shiftTime) {
                var arrivalDate = $("#txt_dateofarrival").val();
                var placeID = $("#ddl_place").val();
                var values = {
                    PlaceID: placeID,
                    ArrivalDate: arrivalDate,
                    ShiftTime: shiftTime
                };

                $.ajax({
                    type: 'POST',
                    contentType: 'application/json; charset=utf-8',
                    data: JSON.stringify(values),
                    url: RootURl + 'OnlineTicketBooking/CheckTicketAvailability',
                    success: function (data) {
                        $('#lbl_NumberofMembersavailable').text("Currently available seats: " + data);
                    },
                    traditional: true,
                    error: function (data) { console.log(data) }
                });
            }
            else { $('#lbl_NumberofMembersavailable').text(""); }
            document.getElementById("errorDate").style.display = 'none';
            
        });
        $("#txt_NumberofMembers").change(function () {
            document.getElementById("errortotMember").style.display = 'none';
        });
        $(document).on('keypress', '#txt_dateofarrival', function (event) {
          
            event.preventDefault();
            return false;

        });
       

        $("#txt_IndAdult").bind("change", function () { return checkMemberCount(this); });

        $("#txt_IndChild").bind("change", function () { return checkMemberCount(this); });

        $("#txt_forAdult").bind("change", function () { return checkMemberCount(this); });

        $("#txt_forChild").bind("change", function () { return checkMemberCount(this); });

        $("#txt_Student").bind("change", function () { return checkMemberCount(this); });

        function checkMemberCount(object) {
            var allmembers = $("#txt_NumberofMembers").val();
            if (!allmembers) { allmembers = 0; }
            var IndianAdultmembers = $("#txt_IndAdult").val();
            if (!IndianAdultmembers) { IndianAdultmembers = 0; }
            var totalIndChild = $("#txt_IndChild").val();
            if (!totalIndChild) { totalIndChild = 0; }
            var ForAdultmembers = $("#txt_forAdult").val();
            if (!ForAdultmembers) { ForAdultmembers = 0; }
            var totalForChild = $("#txt_forChild").val();
            if (!totalForChild) { totalForChild = 0; }
            var totalStudent = $("#txt_Student").val();
            if (!totalStudent) { totalStudent = 0; }
            var members = (parseInt(totalIndChild, 10) + parseInt(IndianAdultmembers, 10) + parseInt(totalForChild, 10) + parseInt(ForAdultmembers, 10) + parseInt(totalStudent, 10));

            if (parseInt(members, 10) <= parseInt(allmembers, 10)) {
            }
            else {
                alert('Value must be less then total members');
                $('#' + object.id).val('');
            }
        }


        $('#btn_done').click(function () {
            if ($('#ddl_Category').val() == '' || $('#ddl_Category').val() == '0') {
                document.getElementById("errorType").style.display = 'block';
                $('html, body').animate({ scrollTop: $("#ddl_Category").offset().top}, 600);
                return false;
            }
            if ($('#ddl_Districts').val() == '' || $('#ddl_Districts').val() == '0') {
                document.getElementById("errorDis").style.display = 'block';
                $('html, body').animate({ scrollTop: $("#ddl_Districts").offset().top }, 600);
                return false;
            }
             
            if ($('#ddl_place').val() == '' || $('#ddl_place').val() == '0') {
                document.getElementById("errorPlace").style.display = 'block';
                $('html, body').animate({ scrollTop: $("#ddl_place").offset().top }, 600);
                return false;
            }

            if ($('#ddl_Shift').val() == '' || $('#ddl_Shift').val() == '0') {
                document.getElementById("errorShift").style.display = 'block';
                $('html, body').animate({ scrollTop: $("#ddl_Shift").offset().top }, 600);
                return false;
            }
              
            if ($('#txt_dateofarrival').val() == '' || $('#txt_dateofarrival').val() == '0') {
                document.getElementById("errorDate").style.display = 'block';
                $('html, body').animate({ scrollTop: $("#txt_dateofarrival").offset().top }, 600);
                return false;
            }
            if ($('#txt_dateofarrival').val() == '' || $('#txt_dateofarrival').val() == '0') {
                document.getElementById("errorDate").style.display = 'block';
                $('html, body').animate({ scrollTop: $("#txt_dateofarrival").offset().top }, 600);
                return false;
            }

                var allmembers = $("#txt_NumberofMembers").val();
                if (allmembers != '') {
                    var IndAdult = $("#txt_IndAdult").val();
                    if (!IndAdult) { IndAdult = 0; }
                    var IndAdultFees = document.getElementById("lbl_IndAdult").textContent;
                    var IndPerAdultFees = IndAdultFees.split(":");
                    var IndianAdultTotalFees = (parseInt(IndAdult, 10) * parseInt(IndPerAdultFees[1], 10));

                    var IndChild = $("#txt_IndChild").val();
                    if (!IndChild) { IndChild = 0; }
                    var IndChildFees = document.getElementById("lbl_IndChild").textContent;
                    var IndPerChildFees = IndChildFees.split(":");
                    var IndianChildTotalFees = (parseInt(IndChild, 10) * parseInt(IndPerChildFees[1], 10));

                    var Student = $("#txt_Student").val();
                    if (!Student) { Student = 0; }
                    var StudentFees = document.getElementById("lbl_Student").textContent;
                    var StudentFees = StudentFees.split(":");
                    var StudentTotalFees = (parseInt(Student, 10) * parseInt(StudentFees[1], 10));

                    var ForAdult = $("#txt_forAdult").val();
                    if (!ForAdult) { ForAdult = 0; }
                    var ForAdultFees = document.getElementById("lbl_ForAdultfee").textContent;
                    var ForPerAdultFees = ForAdultFees.split(":");
                    var ForAdultTotalFees = (parseInt(ForAdult, 10) * parseInt(ForPerAdultFees[1], 10));

                    var ForChild = $("#txt_forChild").val();
                    if (!ForChild) { ForChild = 0; }
                    var ForChildFees = document.getElementById("lbl_ForChildfee").textContent;
                    var ForPerChildFees = ForChildFees.split(":");
                    var ForChildTotalFees = (parseInt(ForChild, 10) * parseInt(ForPerChildFees[1], 10));

                    var tot_members = (parseInt(IndAdult, 10) + parseInt(IndChild, 10) + parseInt(Student, 10) + parseInt(ForAdult, 10) + parseInt(ForChild, 10));
                    if (tot_members == allmembers) {
                        var Guide = $("#txt_guideFee").val();
                        if (!Guide) { Guide = 0; }
                        var GuideFees = document.getElementById("lbl_guidefee").textContent;
                        var PerGuideFees = GuideFees.split(":");
                        var GuideTotalFees = (parseInt(Guide, 10) * parseInt(PerGuideFees[1], 10));

                        var Camera = $("#txt_cameraFees").val();
                        if (!Camera) { Camera = 0; }
                        var CameraFees = document.getElementById("lbl_camerafee").textContent;
                        var PerCameraFees = CameraFees.split(":");
                        var CameraTotalFees = (parseInt(Camera, 10) * parseInt(PerCameraFees[1], 10));

                        var SingleRoom = $("#txt_singleOccupancy").val();
                        if (!SingleRoom) { SingleRoom = 0; }
                        var SingleRoomFees = document.getElementById("lbl_singleOccupancyfee").textContent;
                        var PerSingleRoomFees = SingleRoomFees.split(":");
                        var SingleRoomTotalFees = (parseInt(SingleRoom, 10) * parseInt(PerSingleRoomFees[1], 10));

                        var DoubleRoom = $("#txt_doubleOccupancy").val();
                        if (!DoubleRoom) { DoubleRoom = 0; }
                        var DoubleRoomFees = document.getElementById("lbl_doubleOccupancy").textContent;
                        var PerDoubleRoomFees = DoubleRoomFees.split(":");
                        var DoubleRoomTotalFees = (parseInt(DoubleRoom, 10) * parseInt(PerDoubleRoomFees[1], 10));

                        var Safari = $("#txt_safariMember").val();
                        if (!Safari) { Safari = 0; }
                        var SafariFees = document.getElementById("lbl_safarifee").textContent;
                        var PerSafariFees = SafariFees.split(":");
                        var SafariTotalFees = (parseInt(Safari, 10) * parseInt(PerSafariFees[1], 10));

                        var totalfeesAmount = (parseInt(IndianAdultTotalFees, 10) + parseInt(IndianChildTotalFees, 10) + parseInt(StudentTotalFees, 10)
                            + parseInt(ForAdultTotalFees, 10) + parseInt(ForChildTotalFees, 10) + parseInt(GuideTotalFees, 10) + parseInt(CameraTotalFees, 10)
                             + parseInt(SingleRoomTotalFees, 10) + parseInt(DoubleRoomTotalFees, 10) + parseInt(SafariTotalFees, 10));

                        document.getElementById("txt_finalfees").value = totalfeesAmount;

                        var discount = $("#txt_discount").val();
                        if (!discount) { discount = 0; }
                        var discountedAmount = parseFloat((totalfeesAmount * discount) / 100).toFixed(2);
                        var taxrate = $("#txt_taxRate").val();
                        if (!taxrate) { taxrate = 0; }
                        var taxableAmount = (parseFloat(totalfeesAmount).toFixed(2));
                        var TaxAmount = parseFloat((taxableAmount * taxrate) / 100).toFixed(2);
                        var finalAmount = (parseFloat(taxableAmount) + parseFloat(TaxAmount)) - parseFloat(discountedAmount);
                        document.getElementById("txt_amountTobePaid").value = finalAmount.toFixed(2);
                        $("#div_payment").css("display", "block");
                    }
                    else {
                        alert('Please enter values equal to total members!!');
                    }
                }
                else {
                    document.getElementById("errortotMember").style.display = 'block';
                    $('html, body').animate({ scrollTop: $("#txt_NumberofMembers").offset().top }, 600);
                }
        });

           $("#btn_submit").bind("click", function (e) {
          
            var DistrictsID = $("#ddl_Districts").val();
            var placeID = $("#ddl_place").val();
            var shiftTime = $("#ddl_Shift").val();
            var arrivalDate = $("#txt_dateofarrival").val();
            var totalperson = $("#txt_NumberofMembers").val();
            var IndAdult = $('#txt_IndAdult').val();
            var IndChild = $('#txt_IndChild').val();
            var student = $('#txt_Student').val();
            var FAdult = $('#txt_forAdult').val();
            var FChild = $('#txt_forChild').val();
            var guide = $('#txt_guideFee').val();
            var camera = $('#txt_cameraFees').val();
            var singleOccupancy = $('#txt_singleOccupancy').val();
            var doubleOccupancy = $('#txt_doubleOccupancy').val();
            var safari = $('#txt_safariMember').val();
            var TotalFee = $('#txt_finalfees').val();
            var discount = $('#txt_discount').val();
            var taxRate = $('#txt_taxRate').val();
            var amount = $('#txt_amountTobePaid').val();


            var feesInfo = {
                DistrictID: DistrictsID,
                PlaceID: placeID,
                ShiftTime: shiftTime,
                ArrivalDate: arrivalDate,
                TotalPerson: totalperson,
                IndianAdult: IndAdult,
                IndianChild: IndChild,
                Student: student,
                ForeignerAdult: FAdult,
                ForeignerChild: FChild,
                Guide: guide,
                Camera: camera,
                SingleRoom: singleOccupancy,
                DoubleRoom: doubleOccupancy,
                Safari: safari,
                TotalFees: TotalFee,
                DiscountPercentage: discount,
                TaxRate: taxRate,
                Amount: amount,
                Command: 'Payment',
                Message: ''
            };

            $.ajax({
                type: 'POST',
                contentType: 'application/json; charset=utf-8',
                data: JSON.stringify(feesInfo),
                url: RootURl + 'OnlineTicketBooking/SubmitticketDetails',
                success: function (data) {
                    window.location.href = ""

                },
                traditional: true,
            });
        });
    });
</script>
