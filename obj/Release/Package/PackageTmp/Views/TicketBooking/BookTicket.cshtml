        $("#ddl_vehicle").bind("change", function () {
            var vichel = $("#ddl_vehicle option:selected").text();
            $("#hdn_vehicleID").val($("#ddl_vehicle option:selected").val());
            var Zone = $('#ddl_Zone').val();
            if (!Zone) { Zone = 0; }
            var shift = $("#ddl_Shift").val();
            if (!shift) { shift = 0; }
            var values = {
                VehicleCatID: $("#ddl_Vehiclecat").val(),
                VehicleID: $("#ddl_vehicle").val(),
                ArrivalDate: $("#txt_dateofarrival").val(),
                PlaceID: $("#ddl_place").val(),
                Zone: Zone,
                shift: shift,
            };
            if ($("#ddl_vehicle option:selected").text() != '--Select--') {

                $.ajax({
                    type: 'POST',
                    contentType: 'application/json; charset=utf-8',
                    data: JSON.stringify(values),
                    url: RootURl + 'TicketBooking/SelectVehicleFee',
                    success: function (data) {
                        $.each(data, function (i, items) {
                            if (parseInt(items.AvailableSeat, 10) > parseInt($('#txt_NoOfTicket').val(), 10)) {
                                $('#lbl_feePerVehicle').text("Fees per seat: " + items.FeePerVehicle + " (in INR.)");
                            $('#hdn_vehiclefee').val(items.FeePerVehicle);
                            $('#lbl_vehicleSeatAvailable').text("Currently available seats in " + vichel + ": " + items.AvailableSeat);
                            $('#hdn_vehicleSeatAvailable').val(items.AvailableSeat);
                            }
                            else {
                                alert('No seat avaliable');
                                $("#ddl_vehicle").val(0);
                                $('#lbl_feePerVehicle').text('');
                                $('#hdn_vehiclefee').val('');
                                $('#lbl_vehicleSeatAvailable').text('');
                                $('#hdn_vehicleSeatAvailable').val('');
                                return false;
                            }
                        });
                    },
                    traditional: true,
                    error: function (data) { console.log(data) }
                });

            }
           
        });

