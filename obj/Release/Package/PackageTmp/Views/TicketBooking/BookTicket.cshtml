<script src="~/js/jquery.min.js"></script>
<script type="text/javascript">
    var RootURl = '@Url.Content("~/")';
    $(function () {
        
        if ($('#hdnDeptKiosk').val() != '') {
            $(".datefield").datepicker({ minDate: new Date(),maxDate:"89", dateFormat: 'dd/mm/yy', changeYear: true, onClose: function (dateText, inst) { $("[id$=auto]").focus(); } });
        }
        else {
         // $(".datefield").datepicker({ minDate: "01/10/2016", maxDate: "90", dateFormat: 'dd/mm/yy', changeYear: true, onClose: function (dateText, inst) { $("[id$=auto]").focus(); } });
