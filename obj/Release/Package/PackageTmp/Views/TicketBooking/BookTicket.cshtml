            if (parseInt(noOfTkt) > 6) {
                alert('only 6 seat can be booked at a time!');
                $("#txt_NoOfTicket,#txt_Totalseat").val('');
            }
            if (parseInt(noOfTkt) > parseInt(availTkt)) {
