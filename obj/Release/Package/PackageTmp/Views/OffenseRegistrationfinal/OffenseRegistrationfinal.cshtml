<script>
    $j(document).ready(function () {
        $j('#VechileRTO').change(function () {

            if ($j('#VechileRTO').val() == 'YES') {


                $j('#BtnGetRTODetails').prop("disabled", false);

                $j('#VechileOwnerName').prop("disabled", true);
                $j('#VechileMake').prop("disabled", true);
                $j('#VechileModel').prop("disabled", true);
                $j('#VechileChassisNo').prop("disabled", true);
                $j('#VechileEngineNo').prop("disabled", true);
            }
            else {
                $j('#BtnGetRTODetails').prop("disabled", true);

                $j('#VechileOwnerName').prop("disabled", false);
                $j('#VechileMake').prop("disabled", false);
                $j('#VechileModel').prop("disabled", false);
                $j('#VechileChassisNo').prop("disabled", false);
                $j('#VechileEngineNo').prop("disabled", false);
            }
        });
    });
</script>
<script>
    $j(document).ajaxStart($j.blockUI).ajaxStop($j.unblockUI);
</script>


<script>

    function GetRTODetails() {

        var VechileRegistrationNo = $("#VechileRegistrationNo").val();

        $.ajax({
            type: 'POST',
            url: RootUrl + 'OffenseRegistrationfinal/GetRTOVechileRDetails', // we are calling json method
            dataType: 'json',
            data: { VechileRegistrationNumber: VechileRegistrationNo },
            success: function (ReturnData) {



                $("#VechileOwnerName").val(ReturnData.Rc_owner_name);

                // $("#VechileType").val() = "";
                $("#VechileMake").val(ReturnData.Rc_maker_desc);
                $("#VechileModel").val(ReturnData.Rc_maker_model);
                $("#VechileChassisNo").val(ReturnData.Rc_chasi_no);
                $("#VechileEngineNo").val(ReturnData.Rc_eng_no);





            },
            error: function (ex) {
                alert('Failed to retrieve states.' + ex);
            }

        });
    }
</script>




