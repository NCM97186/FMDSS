<div id="page-wrapper-inn">
    <div class="col-lg-12 pad">
        <h1 class="page-header"><span>Action Request</span></h1>
    </div>
    <div class="row">
        <div class="col-lg-12">
            <div class="panel panel-default">
                <!--<div class="panel-heading">Apply for auction based on the notice with encryption of field</div>-->
                <!-- /.panel-heading -->
                @using (Html.BeginForm("SubmitActionResult", "ForesterAction", FormMethod.Post, new { enctype = "multipart/form-data" }))
                {
                    <div class="panel-body">
                        <div class="col-lg-6">
                            <div class="form-group">
                                <label>Module Name:</label>
                                <input type="text" class="form-control" id="txtModule" readonly="readonly" value="@Model.ModuleName" />
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <div class="form-group">
                                @if (@Model.TableName == "tbl_mst_Notice")
                                {
                                    <label>Request Name:</label>
                                }
                                else
                                {
                                    <label>Service Type:</label>
                                }
                                <input type="text" class="form-control" id="txtServiceType" readonly="readonly" value="@Model.ServiceType" />
                            </div>
                        </div>
                        @if (@Model.TableName != "tbl_mst_Notice")
                        {
                            <div class="col-lg-6">
                                <div class="form-group">
                                    <label>Permission Type:</label>
                                    <input type="text" class="form-control" id="txtPermissionType" readonly="readonly" value="@Model.PermissionType" />
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div class="form-group">
                                    <label>Permission Name:</label>
                                    <input type="text" class="form-control" id="txtPermissionName" readonly="readonly" value="@Model.PermissionName" />
                                </div>
                            </div>
                        }
                        @if (@Model.TableName.Trim() != "Tbl_Citizen_TransitPermit")
                        {
                            <div class="col-lg-6 DF">
                                <div class="form-group">
                                    @if (@Model.TableName == "tbl_mst_Notice")
                                    {
                                        <label>Duration of Notice:</label>
                                    }
                                    else
                                    {
                                        <label>Duration of Permission:</label>
                                    }
                                    <label>From:</label>
                                    @if (@Model.TableName == "tbl_mst_Notice")
                                    {
                                        <input type="text" readonly="readonly" class="form-control datefield" id="durationfrom" name="Durationfrom" value="@Model.DurationsFrom" />
                                    }
                                    else
                                    {
                                        <input type="text" class="form-control datefield" id="durationfrom" name="Durationfrom" value="@Model.DurationsFrom" />
                                    }
                                </div>
                            </div>
                            <div class="col-lg-6 DT">
                                <div class="form-group">
                                    <label>To:</label>
                                    @if (@Model.TableName == "tbl_mst_Notice")
                                    {
                                        <input type="text" readonly="readonly" class="form-control datefield" id="durationto" name="Durationto" value="@Model.DurationsTo" />
                                    }
                                    else
                                    {
                                        <input type="text" class="form-control datefield" id="durationto" name="Durationto" value="@Model.DurationsTo" />
                                    }
                                </div>
                            </div>
                        }
