@model FMDSS.Models.Master.EmitraKioskLinkDirectOrBypass



<script type="text/javascript" src="~/js/jquery.min.js"></script>


<script src="~/Scripts/jquery.validate.min.js" type="text/javascript"></script>
<script src="~/Scripts/jquery.validate.unobtrusive.min.js" type="text/javascript"></script> 


@using (Html.BeginForm("AddUpdateEmitraKioskLinkDirectOrBypass", "Master", FormMethod.Post))
{
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal">
                &times;
            </button>
            <h4 class="modal-title">
                @ViewBag.OpType
            </h4>
        </div>

        <div class="modal-body">
            @Html.HiddenFor(model => model.ID)

            <div class="col-lg-12">
                <div class="form-group">
                    <label>SERVICE NAME: <span class="mandatory">*</span></label>
                    @Html.TextBoxFor(model => model.SERVICENAME, new { @class = "form-control common" })
                   
                    <div id="errJFMC" class="alert-danger1">@Html.ValidationMessageFor(x => x.SERVICENAME)</div> 
                </div>
            </div>
          
            <div class="col-lg-8">
                <div class="form-group">
                    <label>DIRECT LINK: <span class="mandatory">*</span></label>
                    @Html.TextAreaFor(model => model.DIRECT_LINK, new { @class = "form-control common" })

                    <div id="errJFMC" class="alert-danger1">@Html.ValidationMessageFor(x => x.DIRECT_LINK)</div>
                </div>
            </div>
            <div class="col-lg-4">
                <div class="form-group">
                    <label>DIRECT LINK Active / Deactive : <span class="mandatory">*</span></label>
                    @Html.DropDownListFor(Model => Model.DIRECT_LINK_IsActive, (IEnumerable<SelectListItem>)ViewBag.lstISactive,  new { @class = "form-control common" })
                 
                </div>
            </div>

            <div class="col-lg-8">
                <div class="form-group">
                    <label>RAJ SEVADWAR LINK: <span class="mandatory">*</span></label>
                    @Html.TextAreaFor(model => model.RAJSEVADWAR_LINK, new { @class = "form-control common" })
                    <div id="errJFMC" class="alert-danger1">@Html.ValidationMessageFor(x => x.RAJSEVADWAR_LINK)</div>

                </div>
            </div>
            <div class="col-lg-4">
                <div class="form-group">
                    <label>RAJ SEVADWAR Active / Deactive : <span class="mandatory">*</span></label>
                    @Html.DropDownListFor(Model => Model.RAJSEVADWAR_LINK_IsActive, (IEnumerable<SelectListItem>)ViewBag.lstISactive,  new { @class = "form-control common" })
              
                </div>
            </div>
            <div class="col-lg-6">
                <div class="form-group">
                    <label>MAX RESPONSE TIME IN SECOND: <span class="mandatory">*</span></label>
                    @Html.TextBoxFor(model => model.MAX_RESPONSE_TIME_SEC, new { @class = "form-control common" })

                    <div id="errJFMC" class="alert-danger1">@Html.ValidationMessageFor(x => x.MAX_RESPONSE_TIME_SEC)</div>
                </div>
            </div>

            <div class="col-lg-6">
                <div class="form-group">
                    <label>Active / Deactive : <span class="mandatory">*</span></label>
                    @Html.DropDownListFor(Model => Model.IsActive, (IEnumerable<SelectListItem>)ViewBag.lstISactive,  new { @class = "form-control common" })
                   
                </div>
            </div>

            <div class="col-lg-12"></div>
            <!-- /.Btn Groping -->

        </div>
        <div class="modal-footer" style="clear:both">
            <div id="btn" class="col-lg-12">

                <button type="submit" class="btn btn-success" value="Save" id="btn_submit1">
                    <i class="fa fa-check"></i> Save
                </button>

                <button type="button" id="btn_cancel" name="Command" value="Cancel" class="btn btn-danger" data-dismiss="modal">
                    <i class="fa fa-times fa-fw"></i> Close
                </button>

            </div>

        </div>
    </div>

}