</div>
<script type="text/javascript">
    var RootUrl = '@Url.Content("~/")';
    //function CallPlaceData() {
    //    $.ajax({
    //        type: 'GET',
    //        url: RootUrl + 'UserManagementMaster/MultipleOfficeMapping',
    //        dataType: 'html',
    //        success: function (data) {
    //            $('#modelPlace').html(data);
    //        },
    //        error: function (ex) {
    //            alert(ex.error);
    //        }
    //    });



    //};
    $('#btnView').click(function () {
        var SSOID = $("#Ssoid").val();
        $.ajax({
            type: 'GET',
            url: RootUrl + 'UserManagementMaster/_SSODetailMultipleOffice?ssoid=' + SSOID,
            dataType: 'html',
            success: function (data) {
                $('#dvData').html(data);
            },
            error: function (ex) {
                alert(ex.error);
            }
        });
        $("#Ssoid").val('');
    });
</script>