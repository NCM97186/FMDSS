                            <label>Ssoid <span class="mandatory">*</span></label>
                        </div>
                    </div>
                    <div class="col-lg-8">
                        <div class="form-group">
                            @Html.TextBox("Ssoid", null, new { @class = "form-control common", id = "Ssoid" })

