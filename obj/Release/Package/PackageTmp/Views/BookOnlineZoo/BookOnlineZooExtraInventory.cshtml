<script type="text/javascript">
    var RootURl = '@Url.Content("~/")';

    $(document).ready(function () {


        if ($("#hdnDeptKioskStatus").val() == "True") {
            if ('@ViewBag.statusoftime' == 'True') {

                $("select#PlaceOfVisit").prop('selectedIndex', 1);
                var PlaceId = $("#PlaceOfVisit option:selected").val();
                if (!PlaceId) { PlaceId = 0; }


                var values = {
                    PlaceId: PlaceId
                };
                $.ajax({
                    type: 'POST',
                    contentType: 'application/json; charset=utf-8',
                    data: JSON.stringify(values),
                    url: RootURl + 'BookOnlineZoo/ShowPartialViewExtraInventory',
                    success: function (data) {

                        $('#MemberDIV').html(data.MemberPartialView);
                        $('#VehicleDIV').html(data.VehiclePartialView);

                        if (data.VehicleStatus == "TRUE") {
                            $('#VehicleIDSS').show();
                            $("#PrivateVehicle").prop("checked", true);

                        }
                        else {
                            $('#VehicleIDSS').hide();
                            $('#PrivateVehicle').removeProp('checked');
                        }


                        //$.each(data.LstShiftType, function (i, items) {
                        //    $("#ShiftTypeID").append('<option value="' + items.ShiftTypeID + '">' + items.ShiftTypeName + '</option>');
                        //});
                        //$("select#ShiftTypeID").prop('selectedIndex', 1);

                        //$.each(data.LstShiftType, function (i, items) {
                        //    $("#ShiftTypeID").append('<option value="' + items.ShiftTypeID + '">' + items.ShiftTypeName + '</option>');
                        //});

                        $("select#ShiftTypeID").prop('selectedIndex', 1);
                        var ShiftTypeIDs = $("#ShiftTypeID").val();

                        CallingSheet(ShiftTypeIDs);

                    }
                });
                $("#DateOfVisit").val('@ViewBag.CurrentDate');
            }



        }

    });




    function CallingSheet(ShiftTypeIDs) {
        var VisitDate = '@ViewBag.CurrentDate'; //$("#DateOfVisit").val();


        var values = {
            PlaceId: $("#PlaceOfVisit").val(),
            ShiftType: ShiftTypeIDs,
            VisitDate: $("#DateOfVisit").val()
        };
        $.ajax({
            type: 'POST',
            contentType: 'application/json; charset=utf-8',
            data: JSON.stringify(values),
            url: RootURl + 'BookOnlineZoo/CheckTicketAvailability',
            success: function (data) {
                var d = data.split('#');
                if (d[0] != '0') {
                    $('#lbl_AvaliableSeat').text("Currently available seats: " + d[0]);
                    $('#hdAvaliableSeats').val(d[0]);
                }
                if (d[0] == '0') {
                    $('#lbl_AvaliableSeat').text("Seats not available");
                    $('#hdAvaliableSeats').val(d[0]);
                }
            }
        });


    }




    $(document).ready(function () {

